# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Your Supabase project URL
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anonymous/public key (safe to use in frontend)
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_key_here

# Your Supabase project ID
VITE_SUPABASE_PROJECT_ID=your_project_id_here

# Service Role Key (ONLY needed for admin setup script, NEVER commit this!)
# Get this from: Supabase Dashboard > Settings > API > service_role key
# This should NOT be in the .env file for production - only use it locally for admin setup
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here_for_admin_setup_only
